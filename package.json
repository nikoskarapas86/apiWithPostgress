    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@ert78gb/jasmine-ts": "^0.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/nodemon": "^1.19.1",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
